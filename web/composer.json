{
    "name": "slim/slim-skeleton",
    "description": "From slim-skeleton",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
