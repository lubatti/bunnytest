{
    "name": "slim/slim-skeleton",
    "description": "From slim-skeleton",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "doctrine/migrations": "^3.1",
        "doctrine/orm": "^2.8",
        "league/tactician": "^1.1",
        "monolog/monolog": "^2.2",
        "php-amqplib/php-amqplib": "^3.0",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.1",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
